<?xml version="1.0" encoding="UTF-8"?>

<package schemaVersion="1.2" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="PACK.xsd">
  <vendor>Keil</vendor>
  <url>http://www.keil.com/pack/</url>
  <name>SAM4_DFP</name>
  <description>Microchip SAM4 Series Device Support and Examples</description>

  <releases>
    <release version="1.6.1" date="2018-05-07">
      Updated device vendor from Atmel to Microchip.
      Updated emWin examples to emWin V5.46e.
      Updated Board Support LED_*.c files.
      Updated system file with corrected SystemCoreClock initial value to value set by SystemInit function
    </release>
    <release version="1.6.0" date="2017-02-06">
      Updated flash algorithms:
       - Added GPNVM Bytes templates (SAM4xGPNVM.s).
       - Added ATSAM4C_GPNVM.FLM.
       - Updated ATSAM4C_*.FLM.
      Updated SAM4C-EK examples
      Updated emWin examples to emWin V5.36f
      Updated system_SAM4S.c.
    </release>
    <release version="1.5.0" date="2016-07-12">
      emWin:
      - Updated emWin examples to emWin V5.32.
    </release>
    <release version="1.4.0" date="2015-08-28">
      Device Support:
      - Added SAM4CMS4C device support
      - Updated SAM4CM* device support
      - Reorganized SM4CM* folder structure
    </release>
    <release version="1.3.0" date="2015-06-22">
      Device Support:
      - Added SAM4C4C and SAM4CP16C
      - Updated SAM4C device support files and user manual.
      - Corrected ATSAM4C32_2048 Flash algorithm:
        -- set/clear GPNVM bits did not work as expected on this device.
      - Device feature descriptions corrected
      Board Support:
      - Added SAM4S-EK board support
      - Added SAM4S-EK, SAN4E-EK emWin examples.
      - Added emWin LCD Configuration file for FTM280C34D (8-bit Interface).
    </release>
    <release version="1.2.0" date="2014-12-05">
      Added ATSAM4S devices:
      - ATSAM4S2A, ATSAM4S2B, ATSAM4S2C,ATSAM4S4A, ATSAM4S4B, ATSAM4S4C
      Added Board Support bundles
      Corrected ATSAM4N, ATSAM4E flash algorithm
      Updated device support files
    </release>
    <release version="1.1.0" date="2014-09-18">
      Updated flash programming algorithms to use internal oscillator.
    </release>
    <release version="1.0.2" date="2014-03-12">
      New Devices: SAM4C32 and SAM4CM32 Series
    </release>
    <release version="1.0.1" date="2014-02-20">
      New Devices: SAM4CP and SAM4CM Series
      Updated: SAM4C Device Support files
    </release>
    <release version="1.0.0">
      First Release version of SAM4 Device Family Pack.
    </release>
  </releases>

