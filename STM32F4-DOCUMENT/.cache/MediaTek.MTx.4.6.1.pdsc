  <!-- devices section (mandatory for Device Family Packs) -->
  <devices>
  <family Dfamily="MT76x7 Series" Dvendor="MediaTek:129">
    <processor Dcore="Cortex-M4" DcoreVersion="r2p1" Dfpu="1" Dmpu="0" Dendian="Little-endian"/>
	  <compile header="driver/CMSIS/Device/MTK/mt7687/Include/mt7687.h"/>
      <!-- <book      name="doc/SDK on MT7687F API Reference Manual.html"    title="SDK on MT7687F API Reference Manual"/> -->
      <sequences>
        <!-- Override for Pre-Defined DebugCoreStart Sequence -->
        <sequence name="DebugCoreStart">
          <block>
            __var SCS_Addr   = 0xE000E000;
            __var DHCSR_Addr = SCS_Addr + 0xDF0;
            __var flag = 0;
            Write32(DHCSR_Addr, 0xA05F0001);
            flag = Read32(0x20000000);
		  </block>
          <control if="flag != 0x76877697">
		  <block>
				Write32(0x8300F050, 0x76371688);
				Write32(0x8300F050, 0x76371688);
				Write32(0x8300F050, 0x76371688);
				Write32(0x20000000, 0x76877697);
             </block>
          </control>
		  <block>
            Write32(0x83080030, 0x2200);
          </block>
        </sequence>
      </sequences>
      <description>
        MT7687 product family is highly integrated single chip which features an application processor, a low power 1x1 Wi-Fi subsystem and a Power Management Unit.
      </description>
     <!-- ************************  MT7687F  **************************** -->
      <subFamily DsubFamily="MT7687F Series">
        <processor  Dclock="192000000"/>
		<!-- *************************  Device 'MT7687F'  ***************************** -->
		<device Dname="MT7687F">
			<memory id="IROM1"                         start="0x10000000" size="0x00200000" startup="1" default="1"/>
			<memory id="IRAM1"                         start="0x20000000" size="0x00040000" init="0"    default="1"/>
			<memory id="IRAM2"                         start="0x00100000" size="0x00010000" init="0"    default="0"/>				   
			<algorithm name="tools/keil/mt7687/7687_32M_MXIC.FLM" start="0x10000000" size="0x00400000" RAMstart="0x20000000" RAMsize="0x00001000" default="1"/>
		</device>				      
      </subFamily>
    </family>
	<family Dfamily="MT25x3 Series" Dvendor="MediaTek:129">
	<processor Dcore="Cortex-M4" DcoreVersion="r2p1" Dfpu="1" Dmpu="0" Dendian="Little-endian"/>
	  <compile header="driver/CMSIS/Device/MTK/mt2523/Include/mt2523.h"/>	 
      <description>
        The MT2523 HDK is a fully functional demonstration board for IoT and Wearables applications powered by MediaTek MT2523G, an ARM Cortex-M4 core-based microcontroller unit (MCU). It enables rich connectivity features, communication with cloud services and real-time control.
      </description>	   
     <!-- ************************  MT2523x  **************************** -->
      <subFamily DsubFamily="MT2523 Series">
        <processor  Dclock="208000000"/>        
		<!-- *************************  Device 'Larkspur2523'  ***************************** -->
		<device Dname="MT2523x">
			<memory id="IROM1"                         start="0x08000000" size="0x00400000" startup="1" default="1"/>
			<memory id="IRAM1"                         start="0x00000000" size="0x00400000" init="0"    default="1"/>
			<memory id="IRAM2"                         start="0x04008000" size="0x00020000" init="0"    default="0"/>	
			<algorithm name="tools/keil/mt2523/2523_32M_MXIC.FLM" start="0x08000000" size="0x00400000" RAMstart="0x04008000" RAMsize="0x00001000" default="1"/>						  
		</device>						
      </subFamily>
    </family>
  </devices>
  
