   <devices>
      <family Dfamily="MAX3265X" Dvendor="Maxim:23">
         <processor Dcore="Cortex-M4" DcoreVersion="r0p1" Dfpu="FPU" Dmpu="NO_MPU" Dendian="Little-endian" Dclock="120000000" />
         <compile header="Libraries/Device/Maxim/MAX32650/Include/max32650.h" />
         <debug svd="SVD/MAX32650/max32650.svd" />
         <description>
           The MAX3265x device family is designed for wearable and portable medical and fitness applications. The devices contain an ARM Cortex-M4 processor with FPU, execute up to 120MHz and include an ADC, 3MB Flash, 1MB SRAM, external memory interfaces and a versatile set of on-chip peripherals.
         </description>
         <book name="https://datasheets.maximintegrated.com/en/ds/MAX32650.pdf" title="MAX32650 Datasheet"/>
         <book name="Documents/MAX32650_User_Guide.pdf" title="MAX32650 User Guide"/>
         <book name="http://infocenter.arm.com/help/topic/com.arm.doc.dui0553a/DUI0553A_cortex_m4_dgug.pdf" title="Cortex-M4 Generic User Guide"/>
         <feature type="Timer" n="6" m="32" name="32-Bit Timers"/>
         <feature type="DMA" n="6"/>
         <feature type="IOs" n="106"/>
         <feature type="WDT" n="2"/>
         <feature type="RTC" n="32768"/>
         <feature type="ADC" n="10" m="10" name="Sigma-Delta ADC, 7.8ksps, Two 5.5V tolerant inputs, Two 1.2V tolerant inputs"/>
         <feature type="TimerOther" n="16" name="Channel Pulse Train Engine"/>
         <feature type="SDIO" n="1" m="4" name="SD Host Controller (SDHC)"/>
         <feature type="SPI" n="3" m="60000000" name="3 SPI Masters"/>
         <feature type="SPI" n="1" m="30000000" name="1 SPI Slave"/>
         <feature type="I2S" n="1"/>
         <feature type="ExtBus" n="4" name="SPI-XiP RAM for external data/code memory expansion"/>
         <feature type="ExtBus" n="8" name="Hyper Bus and AccelaBus for external data/code memory expansion"/>
         <feature type="ExtBus" n="4" name="SPI-XIP Flash for external code memory expansion"/>
         <feature type="I2C" n="2" name="3 I2C Masters"/>
         <feature type="I2C" n="1" name="1 I2C Slave"/>
         <feature type="UART" n="3" m="4000000"/>
         <feature type="USBD" n="1" name="Hi Speed USB Device"/>
         <feature type="VCC" n="1.08" m="1.32" name="Core Supply Voltage"/>
         <feature type="VCC" n="1.8" m="3.6" name="1.8V - 3.6V VDD with I/O Supply Selectable Between 1.8V and 3.6V"/>
         <feature type="Temp" n="-40" m="85"/>
         <feature type="PowerMode" n="4" name="LP3 Run, LP2 DMA, LP1 Standby, LP0 Stop"/>
         <feature type="CoreOther" n="1" name="CRC Calculation Unit"/>
         <feature type="ClockOther" n="120000000" name="High Frequency Internal Relaxation Oscillator"/>
         <feature type="ClockOther" n="40000000" name="Low Power 40MHz Internal Ring Oscillator"/>
         <feature type="ClockOther" n="7372800" name="7.3728MHz Internal Ring Oscillator"/>

         <device Dname="MAX32650">
            <debugconfig default="swd" clock="1000000"/>
            <compile header="Libraries/Device/Maxim/MAX32650/Include/max32650.h" define="MAX32650"/>
            <memory id="IROM1" start="0x10000000" size="0x00300000" startup="1" default="1"/>
            <memory id="IRAM1" access="rwx" start="0x20000000" size="0x00100000" init="0" default="1"/>
            <algorithm name="Flash/MAX32650.FLM" start="0x10000000" size="0x00300000" default="1" style="Keil" />
            <algorithm  name="FlashIAR/FlashMAX32650.flash" start="0x10000000" size="0x00300000" RAMstart="0x20000000" RAMsize="0x00100000" default="0" style="IAR"/>
            <debug svd="SVD/MAX32650/max32650.svd"/>
         </device>
         <device Dname="MAX32651">
            <debugconfig default="swd" clock="1000000"/>
            <compile header="Libraries/Device/Maxim/MAX32650/Include/max32650.h" define="MAX32650"/>
            <memory id="IROM1" start="0x10000000" size="0x00300000" startup="1" default="1"/>
            <memory id="IRAM1" access="rwx" start="0x20000000" size="0x00100000" init="0" default="1"/>
            <algorithm name="Flash/MAX32650.FLM" start="0x10000000" size="0x00300000" default="1" style="Keil" />
            <algorithm  name="FlashIAR/FlashMAX32650.flash" start="0x10000000" size="0x00300000" RAMstart="0x20000000" RAMsize="0x00100000" default="0" style="IAR"/>
            <debug svd="SVD/MAX32650/max32650.svd"/>
         </device>
         <device Dname="MAX32652">
            <debugconfig default="swd" clock="1000000"/>
            <compile header="Libraries/Device/Maxim/MAX32650/Include/max32650.h" define="MAX32650"/>
            <memory id="IROM1" start="0x10000000" size="0x00300000" startup="1" default="1"/>
            <memory id="IRAM1" access="rwx" start="0x20000000" size="0x00100000" init="0" default="1"/>
            <algorithm name="Flash/MAX32650.FLM" start="0x10000000" size="0x00300000" default="1" style="Keil" />
            <algorithm  name="FlashIAR/FlashMAX32650.flash" start="0x10000000" size="0x00300000" RAMstart="0x20000000" RAMsize="0x00100000" default="0" style="IAR"/>
            <debug svd="SVD/MAX32650/max32650.svd"/>
