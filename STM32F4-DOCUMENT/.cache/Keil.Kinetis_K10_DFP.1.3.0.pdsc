    <!-- Device + CMSIS Conditions -->
    <condition id="K10D5 CMSIS">
      <description>NXP Kinetis K10 50MHz devices and CMSIS-CORE</description>
      <require condition="K10D5"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
