      <accept Dname="MK10DX256xxx10"/>
    </condition>
    <condition id="K10F12">
      <description>NXP Kinetis K10F 120MHz devices</description>
      <require Dvendor="NXP:11"/>
      <accept Dname="MK10FN1M0xxx12"/>
      <accept Dname="MK10FX512xxx12"/>
