        <memory name="SRAM_UPPER" start="0x20000000" size="0x030000" access="rw" default="1"/>
        <algorithm name="arm/MK_P1M0.FLM" start="0x00000000" size="0x00100000" RAMstart="0x20000000" RAMsize="0x00001000" default="1"/>
        <debug svd="MK64F12.xml"/>
        <variant Dvariant="MK64FN1M0VDC12">
          <compile header="fsl_device_registers.h" define="CPU_MK64FN1M0VDC12"/>
          <environment name="uv">
            <CMisc>--C99</CMisc>
            <LMisc>--scatter "./RTE/Device/MK64FN1M0VDC12/MK64FN1M0xxx12_flash.scf"</LMisc>
