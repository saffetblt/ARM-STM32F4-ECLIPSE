      <!-- ************************  Subfamily 'STM32F301'  **************************** -->
      <subFamily DsubFamily="STM32F301">
        <processor Dclock="72000000"/>

        <debugvars configfile="CMSIS/Debug/STM32F301_318.dbgconf">
          __var DbgMCU_CR      = 0x00000007;   // DBGMCU_CR: DBG_SLEEP, DBG_STOP, DBG_STANDBY
          __var DbgMCU_APB1_Fz = 0x00000000;   // DGBMCU_APB1_FZ: All Peripherals Operate as in Normal Mode
          __var DbgMCU_APB2_Fz = 0x00000000;   // DGBMCU_APB2_FZ: All Peripherals Operate as in Normal Mode
          __var TraceClk_Pin   = 0x00040002;   // PE2
          __var TraceD0_Pin    = 0x00040003;   // PE3
          __var TraceD1_Pin    = 0x00040004;   // PE4
          __var TraceD2_Pin    = 0x00040005;   // PE5
          __var TraceD3_Pin    = 0x00040006;   // PE6
        </debugvars>

        <book name="Documentation/DM00094350.pdf" title="STM32F301x6/8 and STM32F318x8 Reference Manual"/>
        <book name="Documentation/DM00093332.pdf" title="STM32F301x6 STM32F301x8 Dataheet"/>
        <book name="Documentation/DM00104412.pdf" title="STM32F301x6 STM32F301x8 Errata Sheet"/>

        <!-- *************************  Device 'STM32F301C6'  ************************** -->
        <device Dname="STM32F301C6">
          <compile header="Drivers/CMSIS/Device/ST/STM32F3xx/Include/stm32f3xx.h" define="STM32F301x8"/>
          <debug svd="CMSIS/SVD/STM32F301x.svd"/>
          <sequences>
            <sequence name="EnableTraceTPIU"  info="ETM not available"> </sequence>
            <sequence name="DisableTraceTPIU" info="ETM not available"> </sequence>
          </sequences>

          <memory id="IROM1"                              start="0x08000000" size="0x00008000" startup="1" default="1"/>
