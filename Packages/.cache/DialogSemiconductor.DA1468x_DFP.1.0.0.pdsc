                <file category="source"  name="Device/Source/ARM/arm_startup_DA1468x.s" attr="config" version="1.0.0" condition="ARM Compiler"/>
				
		        <!-- system file -->
                <file category="sourceC" name="Device/Source/system_DA1468x.c"          attr="config" version="1.0.0"/>
